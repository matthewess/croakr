<% provide :title, "sign-in" %>
<h1>sign-in</h1>
<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <%= bootstrap_form_for :session, url: signin_path do |f| %>
      <!-- %= render 'shared/errors' %-->

      <%= f.email_field :email, label: "email" %>

      <%= f.password_field :password, label: "password" %>

      <%= f.submit "sign-in", class: "btn btn-primary btn-user" %>
    <% end %>
    <p>no account? <%= link_to "sign-up!", signup_path %></p>
  </div>
</div>
